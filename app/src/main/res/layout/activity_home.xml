<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#2d2d2d"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <view
        class="android.support.v7.widget.ActionMenuView"
        id="@+id/view3"
        layout_below="@+id/textView12"
        layout_centerHorizontal="true"
        layout_marginTop="89dp"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#2d2d2d"
        android:textAlignment="center" />

    <view
        android:id="@+id/view"
        class="android.support.v7.widget.ActionMenuView"
        id="@+id/view2"
        layout_alignParentStart="true"
        layout_alignParentTop="true"
        layout_marginStart="152dp"
        layout_marginTop="170dp"
        android:layout_width="260dp"
        android:layout_height="260dp"
        android:layout_below="@+id/textView10"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="20dp"
        android:background="@drawable/box" />

    <View
        android:layout_width="255dp"
        android:layout_height="2dp"
        android:layout_below="@+id/textView4"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="16dp"
        android:background="#e8e8e8"
        android:id="@+id/view4" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="81dp"
        android:fontFamily="@font/opensansregular"
        android:text="Good Morning,"
        android:textColor="@color/colorFontPrimary"
        android:textSize="36sp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView12"
        android:layout_alignTop="@+id/textView11"
        android:layout_marginStart="14dp"
        android:layout_marginTop="17dp"
        android:fontFamily="@font/oswaldregular"
        android:lineSpacingExtra="12sp"
        android:text="Dataran Pahlawan"
        android:textColor="@android:color/black"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView12"
        android:layout_centerHorizontal="true"
        android:fontFamily="@font/opensansregular"
        android:text="Mohamad Zulhilmi"
        android:textAlignment="center"
        android:textColor="@android:color/white"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView4"
        android:layout_alignTop="@+id/view"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/opensansregular"
        android:text="Park Location"
        android:textSize="16sp" />

    <ImageView
        android:id="@+id/imageView3"
        app:srcCompat="@drawable/logo"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignBottom="@+id/textView4"
        android:layout_marginStart="12dp"
        android:layout_toEndOf="@+id/textView17" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView4"
        android:layout_below="@+id/view4"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/opensansregular"
        android:text="Plat No"
        android:textSize="16dp" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView13"
        android:layout_below="@+id/textView13"
        android:fontFamily="@font/oswaldregular"
        android:text="CDM 2003"
        android:textColor="#667eea"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView14"
        android:layout_below="@+id/textView14"
        android:layout_marginTop="6dp"
        android:fontFamily="@font/opensansregular"
        android:text="Slot"
        android:textSize="16dp" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView15"
        android:layout_alignTop="@+id/textView15"
        android:layout_marginTop="18dp"
        android:fontFamily="@font/oswaldregular"
        android:text="027"
        android:textColor="#667eea"
        android:textSize="20sp" />

    <view
        class="android.support.v7.widget.ActionMenuView"
        id="@+id/view5"
        layout_alignBottom="@+id/view"
        layout_marginBottom="36dp"
        layout_marginEnd="12dp"
        layout_toStartOf="@+id/textView10"
        android:layout_width="260dp"
        android:layout_height="50dp"
        android:layout_alignBottom="@+id/view"
        android:layout_alignStart="@+id/view"
        android:background="@drawable/gradient2"
        android:id="@+id/view6" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/view6"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp"
        android:fontFamily="@font/robotoregular"
        android:text="In Progress"
        android:textAlignment="textStart"
        android:textColor="@android:color/white"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/textView10"
        android:layout_alignTop="@+id/textView13"
        android:layout_marginEnd="8dp"
        android:text="Duration"
        android:textSize="16dp" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView14"
        android:layout_alignBottom="@+id/textView14"
        android:layout_alignStart="@+id/textView18"
        android:fontFamily="@font/oswaldregular"
        android:text="7 MIN"
        android:textColor="#667eea"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/-"
        android:layout_alignBottom="@+id/-"
        android:layout_toEndOf="@+id/textView14"
        android:fontFamily="@font/oswaldregular"
        android:text="03:10AM"
        android:textSize="18sp"
        android:textStyle="bold"
        android:typeface="normal" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView16"
        android:layout_toEndOf="@+id/textView20"
        android:fontFamily="@font/oswaldregular"
        android:text="03:15AM"
        android:textSize="18sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/-"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView22"
        android:layout_alignBottom="@+id/textView22"
        android:layout_alignEnd="@+id/textView17"
        android:layout_marginEnd="8dp"
        android:fontFamily="@font/oswaldregular"
        android:text="-"
        android:textSize="18sp"
        android:textStyle="bold" />

    <Button
        android:id="@+id/button2"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/view"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="44dp"
        android:background="@drawable/button_background3"
        android:fontFamily="@font/opensansregular"
        android:text="Parking Now"
        android:textColor="@android:color/white"
        android:textSize="18sp"
        android:textStyle="bold" />


</RelativeLayout>